{
  "manifest_version": 2,

  "name": "Rate Extensions",
  "description": "A Chrome extension that gathers security information about all other installed extensions.",
  "version": "1.0",
  
  "background": {
  	"scripts":["js/jquery-1.10.1.min.js", "js/background.js"]
  },

  "permissions": [
    "tabs",
    "management",
    "http://rate-extension.appspot.com/api/v1/*"

  ],
  "browser_action": {
    "default_icon": "/img/icon.png"
  }
}